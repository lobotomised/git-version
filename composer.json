{
  "name": "lobotomised/git-version",
  "description": "Add git commit id as a blade directives",
  "keywords": ["laravel", "database", "backup"],
  "license": "MIT",
  "authors": [
    {
      "name": "lobotomised",
      "email": "contact@lobotomised.net"
    }
  ],
  "require": {
    "php": "^7.2",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0|^6.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "illuminate/filesystem": "^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Lobotomised\\Gitversion\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lobotomised\\Gitversion\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lobotomised\\Gitversion\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
